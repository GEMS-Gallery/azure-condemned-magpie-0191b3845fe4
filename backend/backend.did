type Result = 
 variant {
   err: text;
   ok: nat;
 };
type File = 
 record {
   category: text;
   fileType: text;
   id: nat;
   name: text;
   size: opt text;
 };
service : {
  addFile: (text, text, opt text, text) -> (Result);
  getFiles: () -> (vec File) query;
  getFilesByCategory: (text) -> (vec File) query;
}
